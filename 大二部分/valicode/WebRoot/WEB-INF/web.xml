<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>NewCode21</servlet-name>
    <servlet-class>com.gec.l21.NewCode</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ValiCode21</servlet-name>
    <servlet-class>com.gec.l21.ValiCode</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>NewCode31</servlet-name>
    <servlet-class>com.gec.l31.NewCode</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ValiCode31</servlet-name>
    <servlet-class>com.gec.l31.ValiCode</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>NewCode21</servlet-name>
    <url-pattern>/code21</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ValiCode21</servlet-name>
    <url-pattern>/vc21</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewCode31</servlet-name>
    <url-pattern>/code31</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ValiCode31</servlet-name>
    <url-pattern>/vc31</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
